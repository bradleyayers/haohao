{
  "buildCommand": "moon run build-web",
  "outputDirectory": "dist",
  "devCommand": "moon run dev",
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.0.11",
      "includeFiles": "dist/server/**"
    }
  },
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
