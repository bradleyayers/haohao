{
  "buildCommand": "moon run build-web",
  "outputDirectory": "dist/client",
  "devCommand": "moon run dev",
  "functions": {
    "api/index.cjs": {
      "runtime": "@vercel/node@3.2.1",
      "includeFiles": "dist/server/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.cjs"
    }
  ]
}
