{
  "extends": "../../tsconfig.base",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["DOM", "ESNext"],
    "module": "ES2020",
    "moduleResolution": "Bundler",
    "noPropertyAccessFromIndexSignature": true,
    "outDir": "../../.moon/cache/types/projects/app",
    "paths": {
      "@/*": ["src/*"],
      "@haohaohow/lib/*": ["../lib/src/*"]
    },
    "resolveJsonModule": true,
    "target": "ESNext"
  },
  "include": [
    "*",
    "src/**/*",
    "api/**/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ],
  "exclude": ["dist", "node_modules", "bin"],
  "references": [
    {
      "path": "../lib"
    }
  ]
}
