{
  "extends": "../../tsconfig.base",
  "ts-node": {
    // Include `.d.ts` files (e.g. `src/typings/*`)
    "files": true
  },
  "compilerOptions": {
    "outDir": "../../.moon/cache/types/projects/static",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "allowJs": true,
    "esModuleInterop": true,
    "lib": ["DOM", "ESNext"],
    "noEmit": true,
    "noPropertyAccessFromIndexSignature": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ESNext"
  }
}
