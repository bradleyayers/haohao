{
  "extends": "../../tsconfig.base",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "lib": ["ESNext"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "noEmit": true,
    "outDir": "../../.moon/cache/types/projects/api",
    "paths": {
      "@/*": ["api/*"]
    },
    "skipLibCheck": true,
    "target": "ESNext",
    "types": ["node"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.mts",
    "**/*.js",
    "**/*.cjs",
    "**/*.mjs"
  ],
  "exclude": ["dist", "node_modules"]
}
