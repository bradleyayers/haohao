{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "allowJs": true,
    "lib": ["ESNext"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "outDir": "../../.moon/cache/types/projects/api"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.mts",
    "src/**/*.js",
    "src/**/*.cjs",
    "src/**/*.mjs",
    "*.ts",
    "*.tsx",
    "*.mts",
    "*.js",
    "*.cjs",
    "*.mjs",
    ".next/types/**/*.ts"
  ],
  "exclude": ["dist", "node_modules"],
  "references": [
    {
      "path": "../lib"
    }
  ]
}
